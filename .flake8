[flake8]
max-line-length = 88
extend-ignore = E203,W503,E501
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .tox,
    .mypy_cache,
    migrations,
    node_modules,
    instance,
    docs
per-file-ignores = 
    __init__.py:F401
max-complexity = 10 